<manifest xmlns:android="http://schemas.android.com/apk/res/android" package="com.zf.graduation">

    <application android:allowBackup="true" android:label="@string/app_name"
        android:icon="@mipmap/ic_launcher" >
    <activity android:name=".ui.MainActivity" android:theme="@style/AppTheme">
        <intent-filter>
            <action android:name="android.intent.action.MAIN"/>
            <category android:name="android.intent.category.DEFAULT"/>
        </intent-filter>
    </activity>
    <activity android:name=".ui.ActivityActivity" android:theme="@style/AppTheme"></activity>
    <activity android:name=".ui.GroupActivity" android:theme="@style/AppTheme"></activity>
    <activity android:name=".ui.MessageActivity" android:theme="@style/AppTheme"></activity>
    <activity android:name=".ui.SetUpActivity" android:theme="@style/AppTheme"></activity>
    <activity android:name=".ui.CreateActivityActivity" android:theme="@style/AppTheme"></activity>
    <activity android:name=".ui.CreateGroupActivity" android:theme="@style/AppTheme"></activity>
    <activity android:name=".ui.SampleActivity" android:theme="@style/MaterialTheme">
        <intent-filter>
            <action android:name="android.intent.action.MAIN"/>
            <category android:name="android.intent.category.LAUNCHER"/>
        </intent-filter>
    </activity>
    </application>

</manifest>
